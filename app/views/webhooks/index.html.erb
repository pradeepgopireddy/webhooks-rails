<p id="notice"><%= notice %></p>

<h1>Webhooks</h1>

<table>
  <thead>
    <tr>
      <th>Source</th>
      <th>Data</th>
      <th>Status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @webhooks.each do |webhook| %>
      <tr>
        <td><%= webhook.source_name %></td>
        <td><%= webhook.data %></td>
        <td><%= webhook.status %></td>
        <td><%= link_to 'Show', webhook %></td>
        <td><%= link_to 'Destroy', webhook, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
